.order-ticket-container {
    padding: 0 30rpx;
    padding-bottom: 140rpx;
    box-sizing: border-box;
    width: 100%;
}

.unusable-container {
    color: #5D5D5D;
    font-size: 24rpx;
    border-bottom: 1rpx solid #D0D0D0;
    margin-top: 20rpx;
    margin-bottom: 20rpx;
    padding-bottom: 20rpx;
}

.user-none {
    width: 100%;
    height: 120rpx;
    border-top: 1rpx solid #D0D0D0;
    position: fixed;
    left: 0;
    bottom: 0;
    text-align: center;
    line-height: 120rpx;
    color: #909090;
    font-size: 26rpx;
    background-color: #fff;
    z-index: 999;
}