page {
    background-color: #F2F3F4;
}

.user-header {
    height: 277rpx;
    width: 100%;
    background-color: #fff;
    position: relative;
}

.user-back {
    width: 100%;
    height: 277rpx;
}

.user-head {
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.user-info {
    display: flex;
    justify-content: center;
    align-items: center;
}

.user-sex {
    width: 22rpx;
    height: 22rpx;
    margin-left: 20rpx;
}

.logout {
    position: absolute;
    top: 27rpx;
    right: 20rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 30rpx;
    color: #5D5D5D;
}

.logout-img {
    width: 27rpx;
    height: 29rpx;
    margin-right: 14rpx;
}

.user-manage-title {
    height: 72rpx;
    padding: 0 30rpx;
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #fff;
    box-sizing: border-box;
}

.title-item {
    width: 33%;
    text-align: center;
    line-height: 72rpx;
    box-sizing: border-box;
    font-size: 32rpx;
    color: #5D5D5D;
    font-weight: 300;
}

.title-item.active {
    color: #EA4D45;
    border-bottom: 4rpx solid #EA4D45;
}

.user-manage-item {
    margin-top: 20rpx;
}

.user-manage-blank {
    margin: 20% 0 27%;
    text-align: center;
    color: #C0C0C0;
    font-size: 32rpx;
    font-weight: 300;
}

.bottom-msg {
    margin-top: 50rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.msg {
    font-size: 24rpx;
    color: #B4B4B4;
}

.logo-img {
    width: 104rpx;
    height: 33rpx;
    margin-top: 36rpx;
    margin-bottom: 70rpx;
}

.ship-blank {
    width: 390rpx;
    height: 230rpx;
    display: block;
    margin: 25% auto 2%;
}

.user-coupon-main {
    background-color: #fff;
    height: 100rpx;
    width: 100%;
    padding: 0 30rpx;
    box-sizing: border-box;
}

.user-coupon {
    height: 86rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 32rpx;
    color: #333333;
    border-bottom: 1rpx solid #D0D0D0;
    box-sizing: border-box;
}

.coupon-text-sorr {
    width: 13rpx;
    height: 23rpx;
    margin-left: 20rpx;
    vertical-align: middle;
}

.coupon-text {
    color: #EE5A52;
    font-size: 24rpx;
}

.nouseful {
    color: #909090;
}